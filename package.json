{
  "name": "firebase_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --list-devices",
    "ios": "react-native run-ios --simulator='iPhone 16 Pro'",
    "lint": "eslint .",
    "start": "react-native start --reset-cache --client-logs",
    "pods": "cd ios && bundle install && bundle exec pod install && cd ..",
    "ipp": "yarn install && npm run patchPods",
    "deletePodlock": "cd ios && rm Podfile.lock && cd ..",
    "deleteYarnlock": "rm yarn.lock",
    "deleteGradleFiles": "cd android && rm -rf .gradle && rm -rd .idea && cd ..",
    "cleanProject": "npx react-native clean && npm run deleteYarnlock && npm run deletePodlock && npm run deleteGradleFiles",
    "cleanAndroid": "cd android && ./gradlew clean && deleteGradleFiles",
    "cleanPods": "cd ios && pod deintegrate && rm Podfile.lock && cd ..",
    "build:android": "cd android && ./gradlew assembleRelease && cd ..",
    "patchPods": "npx patch-package && npm run pods",
    "test": "jest",
    "postinstall": "npx patch-package"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "~5.1.6",
    "@react-native-async-storage/async-storage": "~2.2.0",
    "@react-native-firebase/analytics": "^22.4.0",
    "@react-native-firebase/app": "^22.4.0",
    "@react-native-firebase/auth": "^22.4.0",
    "@react-native-firebase/crashlytics": "^22.4.0",
    "@react-native-firebase/firestore": "^22.4.0",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-native-masked-view/masked-view": "~0.3.2",
    "@react-native-vector-icons/material-design-icons": "~12.0.0",
    "@react-native/new-app-screen": "~0.80.0",
    "@react-navigation/bottom-tabs": "~7.3.14",
    "@react-navigation/native": "~7.1.10",
    "@react-navigation/native-stack": "~7.3.14",
    "@reduxjs/toolkit": "~2.8.2",
    "@tanstack/react-query": "~5.80.7",
    "axios": "~1.9.0",
    "react": "19.0.0",
    "react-hook-form": "~7.57.0",
    "react-native": "0.79.3",
    "react-native-gesture-handler": "~2.25.0",
    "react-native-linear-gradient": "~2.8.3",
    "react-native-pager-view": "~6.8.1",
    "react-native-paper": "~5.14.5",
    "react-native-reanimated": "~3.18.0",
    "react-native-safe-area-context": "~5.4.1",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "~15.12.0",
    "react-native-toast-message": "~2.3.0",
    "react-redux": "~9.2.0"
  },
  "devDependencies": {
    "@babel/core": "~7.27.4",
    "@babel/preset-env": "~7.27.2",
    "@babel/runtime": "~7.27.6",
    "@react-native-community/cli": "~18.0.0",
    "@react-native-community/cli-platform-android": "~18.0.0",
    "@react-native-community/cli-platform-ios": "~18.0.0",
    "@react-native/babel-preset": "0.79.3",
    "@react-native/eslint-config": "0.79.3",
    "@react-native/metro-config": "0.79.3",
    "@react-native/typescript-config": "0.79.3",
    "@types/jest": "~29.5.14",
    "@types/react": "~19.1.8",
    "@types/react-test-renderer": "~19.0.0",
    "babel-jest": "~30.0.0",
    "babel-plugin-module-resolver": "~5.0.2",
    "eslint": "~9.28.0",
    "jest": "~30.0.0",
    "prettier": "~3.5.3",
    "react-native-svg-transformer": "~1.5.1",
    "react-test-renderer": "~19.0.0",
    "typescript": "~5.8.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.9.2"
}
